{
  "name": "i18n-pro",
  "version": "1.3.1",
  "description": "轻量、简单、灵活的自动翻译工具",
  "files": [
    "dist",
    "template",
    "i18n/langs.json",
    "./index.js"
  ],
  "types": "./dist/lib/index.d.ts",
  "main": "./index.js",
  "bin": {
    "i18n": "dist/bin/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "npx tsc --watch",
    "test-init": "node ./dist/bin/index.js init",
    "test-t": "node ./dist/bin/index.js t",
    "test-translate": "node ./dist/bin/index.js translate",
    "build": "npx tsc && rimraf dist/lib && rollup -c",
    "publish": "npm run build && npm publish --access=public --registry https://registry.npmjs.org",
    "lint-code": "npx lint-staged",
    "lint-msg": "npx commitlint --edit",
    "test": "cross-env NODE_ENV=src vitest --ui",
    "test:pkg": "cross-env NODE_ENV=pkg vitest run",
    "coverage": "cross-env NODE_ENV=src vitest run --coverage"
  },
  "keywords": [
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "l10n",
    "localization",
    "translate",
    "translation",
    "国际化",
    "多语言",
    "自动翻译"
  ],
  "author": "Eyelly Wu",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@eyelly/prettier-config": "^1.0.0",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitest/coverage-istanbul": "^0.22.0",
    "@vitest/ui": "^0.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "8",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-prettier": "^2.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "tslib": "^2.4.0",
    "typescript": "4",
    "vitest": "^0.22.0"
  },
  "lint-staged": {
    "*.{json,ts}": "eslint --fix -f table"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyelly-wu/i18n-pro.git"
  },
  "homepage": "https://github.com/eyelly-wu/i18n-pro#readme",
  "bugs": {
    "url": "https://github.com/eyelly-wu/i18n-pro/issues"
  },
  "dependencies": {
    "md5-node": "^1.0.1"
  }
}
