{
  "name": "i18n-pro",
  "version": "1.3.2",
  "description": "Lightweight, simple, flexible and automatic-translation tools for internationalization（轻量、简单、灵活、自动翻译的国际化工具）",
  "files": [
    "dist/src",
    "template",
    "i18n/langs.json",
    "./index.js"
  ],
  "types": "./dist/src/lib/index.d.ts",
  "main": "./index.js",
  "bin": {
    "i18n": "dist/src/bin/index.js"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "npx tsc --watch & jtm run",
    "docs": "jtm run",
    "docs-translate": "node ./dist/src/bin/index.js t -P /Users/Program/Git/MINE_LIB_SOURCING/i18n-pro/docs",
    "test-init": "node ./dist/src/bin/index.js init",
    "test-t": "node ./dist/bin/index.js t",
    "test-translate": "node ./dist/src/bin/index.js translate",
    "build": "npx tsc && rimraf dist/lib && rollup -c",
    "publish": "npm run build && npm publish --access=public --registry https://registry.npmjs.org",
    "lint-code": "npx lint-staged",
    "lint-msg": "npx commitlint --edit",
    "test": "cross-env NODE_ENV=src vitest --ui",
    "test:pkg": "cross-env NODE_ENV=pkg vitest run",
    "coverage": "cross-env NODE_ENV=src vitest run --coverage"
  },
  "keywords": [
    "intl",
    "i18n",
    "internationalization",
    "locale",
    "l10n",
    "localization",
    "translate",
    "translation",
    "国际化",
    "多语言",
    "自动翻译",
    "automatic translation"
  ],
  "author": "Eyelly Wu",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@eyelly/prettier-config": "^1.0.0",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitest/coverage-istanbul": "^0.22.1",
    "@vitest/ui": "^0.22.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-prettier": "^2.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.33.0",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vitest": "^0.22.1"
  },
  "lint-staged": {
    "*.{json,ts}": "eslint --fix -f table"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eyelly-wu/i18n-pro.git"
  },
  "homepage": "https://github.com/eyelly-wu/i18n-pro#readme",
  "bugs": {
    "url": "https://github.com/eyelly-wu/i18n-pro/issues"
  },
  "dependencies": {
    "jsx-to-md": "0.2.0",
    "md5-node": "^1.0.1"
  }
}
